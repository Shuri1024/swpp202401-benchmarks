define dso_local i32 @countSetBits(i32 noundef %n) #0 {
entry:
  %cmp = icmp eq i32 %n, 0
  br i1 %cmp, label %if.then, label %if.else

if.then:                                          ; preds = %entry
  br label %return

if.else:                                          ; preds = %entry
  %and = and i32 %n, 1
  %shr = ashr i32 %n, 1
  %call = call i32 @countSetBits(i32 noundef %shr)
  %add = add nsw i32 %and, %call
  br label %return

return:                                           ; preds = %if.else, %if.then
  %retval.0 = phi i32 [ 0, %if.then ], [ %add, %if.else ]
  ret i32 %retval.0
}

; Function Attrs: nounwind uwtable
define dso_local i32 @main() #0 {
entry:
